#include<iostream>

using namespace std;

int main()

{

    int len,n,w[10000],h[10000];

    cin>>len>>n;

    for(int i=0;i<n;i++)

    {

        cin>>w[i]>>h[i];

    }

    for(int i=0 ;i<n;i++)

    {

    if(w[i]<len||h[i]<len)

    {

        cout<<"UPLOAD ANOTHER\n";

    }

    else if (w[i]==h[i])

    {

        cout<<"ACCEPTED\n";

    }

    else

    {

        cout<<"CROP IT\n";

    }

    }

    return 0;

}
