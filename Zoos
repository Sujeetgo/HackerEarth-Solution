#include <bits/stdc++.h>
using namespace std;
int main() {
	string s;
	cin>>s;
	int x_cnt = 0;
	int y_cnt = 0;
	int n = s.size();
	if(n<=20){
		for(int i = 0; i < n; i++){
			if(s[i] =='z'){
				x_cnt +=1;
			}
			if(s[i]=='o'){
				y_cnt +=1;
			}
		}
		if( 2*x_cnt== y_cnt){
			cout<<"Yes";
		}
		else{
			cout<<"No";
		}
		}

}
